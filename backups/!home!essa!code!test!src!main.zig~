const raylib = @import("raylib");

pub fn main() void {
    raylib.initWindow(800, 600, "This is a window");
    defer raylib.closeWindow();
    while (!raylib.windowShouldClose()) {
        raylib.beginDrawing();

        raylib.clearBackground(.white);
        raylib.drawRectangle(300, 100, 100, 500, .red);
        raylib.endDrawing();
    }
}
