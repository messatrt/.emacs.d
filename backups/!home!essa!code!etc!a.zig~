
const std = @import("std");
const a = std.io.bufferedReader();
    kfeka;;
pub fn main() !void {
    try printFiles();
}

fn printFiles() !void {
    var files = try std.fs.cwd().openDir(".", .{ .iterate = true });
    defer files.close();
    var it = files.iterate();
    while (try it.next()) |e| {
        std.debug.print("{s}\n", .{e.name});
    }
}
